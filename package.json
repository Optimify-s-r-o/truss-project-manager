{
  "name": "truss-project-manager",
  "version": "0.1.16",
  "productName": "Truss Project Manager",
  "description": "Truss Project Manager",
  "main": "public/electron.js",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/pro-duotone-svg-icons": "^5.15.3",
    "@fortawesome/pro-light-svg-icons": "^5.15.3",
    "@fortawesome/pro-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@microsoft/signalr": "^5.0.5",
    "@microsoft/signalr-protocol-msgpack": "^5.0.5",
    "antd": "^4.15.3",
    "blob-to-buffer": "^1.2.9",
    "connected-react-router": "^6.9.1",
    "date-fns": "^2.21.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-localshortcut": "^3.2.1",
    "electron-log": "^4.3.4",
    "electron-store": "^7.0.3",
    "electron-updater": "^4.3.8",
    "electron-window-state": "^5.0.3",
    "formik": "^2.2.6",
    "history": "^4.10.1",
    "i18next": "^20.2.2",
    "i18next-browser-languagedetector": "^6.1.0",
    "is-electron": "^2.2.0",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-collapsible": "^2.8.3",
    "react-currency-format": "^1.0.0",
    "react-currency-input": "^1.3.6",
    "react-date-range": "^1.1.3",
    "react-datepicker": "^3.8.0",
    "react-dev-utils": "^11.0.4",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.3.2",
    "react-export-excel": "^0.5.3",
    "react-i18next": "^11.8.15",
    "react-key-handler": "^1.2.0-beta.3",
    "react-keyboard-event-handler": "^1.5.4",
    "react-moment": "^1.1.1",
    "react-onclickoutside": "^6.10.0",
    "react-phone-number-input": "^3.1.20",
    "react-redux": "^7.2.4",
    "react-resize-aware": "^3.1.0",
    "react-responsive-carousel": "^3.2.18",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-select": "^4.3.0",
    "react-toast-notifications": "^2.4.4",
    "react-tooltip": "^4.2.18",
    "redux": "^4.1.0",
    "redux-persist": "^4",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.3",
    "temp": "^0.9.4",
    "typesafe-actions": "^5.1.0",
    "universal-cookie": "^4.0.4",
    "web-vitals": "^1.1.1",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "env-cmd -f .env.prod react-scripts start",
    "start:local": "env-cmd -f .env.local react-scripts start",
    "build": "env-cmd -f .env.prod react-scripts build",
    "build:dev": "env-cmd -f .env.dev react-scripts build",
    "build:prod": "env-cmd -f .env.prod react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "local": "concurrently -k \"npm run start:local\" \"npm:electron\"",
    "dev": "concurrently -k \"npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "release": "npm run build:prod && electron-builder build --win -p always",
    "release:dev": "npm run build:dev && electron-builder build --win -p always"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "jsx-a11y/anchor-is-valid": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "artifactName": "${productName}-Setup-${version}.${ext}",
    "appId": "org.common.tpm",
    "copyright": "Copyright Â© 2018-2020 Optimify s.r.o.",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "./public/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "Optimify-s-r-o",
        "repo": "truss-project-manager",
        "releaseType": "draft"
      }
    ],
    "win": {
      "artifactName": "Truss Project Manager Setup ${version}.${ext}",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "publish": [
        "github"
      ],
      "icon": "./public/icon.ico"
    },
    "nsis": {
      "artifactName": "${productName}-Setup-${version}.${ext}",
      "include": "resources/installer-prod.nsh",
      "warningsAsErrors": false,
      "shortcutName": "Truss Project Manager",
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "deleteAppDataOnUninstall": true,
      "createStartMenuShortcut": true
    },
    "directories": {
      "buildResources": "public"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Optimify-s-r-o/truss-project-manager"
  },
  "author": {
    "name": "Optimify s.r.o.",
    "email": "dev@optimify.cz",
    "url": "https://optimify.cz"
  },
  "devDependencies": {
    "@types/googlemaps": "^3.43.3",
    "@types/history": "^4.7.8",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.4",
    "@types/react-datepicker": "^3.1.8",
    "@types/react-dom": "^17.0.3",
    "@types/react-redux": "^7.1.16",
    "@types/react-router": "^5.1.13",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-select": "^4.0.15",
    "@types/react-test-renderer": "^17.0.1",
    "@types/react-toast-notifications": "^2.4.0",
    "@types/redux-logger": "^3.0.8",
    "@types/styled-components": "^5.1.9",
    "concurrently": "^6.0.2",
    "electron": "^12.0.5",
    "electron-builder": "^22.10.5",
    "env-cmd": "^10.1.0",
    "redux-logger": "^3.0.6",
    "typescript": "^4.2.4",
    "wait-on": "^5.3.0"
  },
  "resolutions": {
    "react-scripts/eslint-webpack-plugin": "2.3.0"
  }
}
